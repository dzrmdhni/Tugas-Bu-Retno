# include <iostream>
using namespace std;
int main ()
{
  int romawi;  
 cout<<" Masukan Data: ";
 cin>>romawi;
   if (romawi==I)
   {
   cout<<"1"<<endl;
}
else if (romawi==II){cout<<"2"<<endl;}

else if (romawi==III){cout<<"3"<<endl;}

else if (romawi==IV){cout<<"4"<<endl;}

else if (romawi==V){cout<<"5"<<endl;}

else if (romawi==VI){cout<<"6"<<endl;}

else if (romawi==VII){cout<<"7"<<endl;}

else if (romawi==VIII){cout<<"8"<<endl;}

else if (romawi==IX){cout<<"9"<<endl;}

return 0;
}
